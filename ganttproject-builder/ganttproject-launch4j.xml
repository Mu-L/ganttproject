<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar>eclipsito.jar</jar>
  <outfile>ganttproject.exe</outfile>
  <errTitle></errTitle>
  <cmdLine>--verbosity 1 --version-dirs plugins;~/.ganttproject.d/updates --app net.sourceforge.ganttproject.GanttProject</cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>https://bell-sw.com/pages/downloads/#/java-17-lts</downloadUrl>
  <supportUrl>https://help.ganttproject.biz</supportUrl>
  <stayAlive>true</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>ganttproject.ico</icon>
  <classPath>
    <mainClass>com.bardsoftware.eclipsito.Launch</mainClass>
    <cp>%EXEDIR%;%EXEDIR%\eclipsito.jar;%EXEDIR%\lib\slf4j-api-2.0.4.jar;%EXEDIR%\lib\slf4j-jdk14-2.0.4.jar</cp>
    <cp>%EXEDIR%\lib\logback-classic-1.5.18.jar</cp>
    <cp>%EXEDIR%\lib\logback-core-1.5.18.jar</cp>
  </classPath>
  <jre>
    <path>.\runtime\</path>
    <requiresJdk>false</requiresJdk>
    <requires64Bit>true</requires64Bit>
    <minVersion>21</minVersion>
    <maxVersion></maxVersion>
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>2048</maxHeapSize>
  </jre>
  <versionInfo>
    <fileVersion>3.3.3390.0</fileVersion>
    <txtFileVersion>GanttProject 3.4 Beta</txtFileVersion>
    <fileDescription>GanttProject Launcher</fileDescription>
    <copyright>Copyright (C) 2025 BarD Software s.r.o.</copyright>
    <productVersion>3.3.3390.0</productVersion>
    <txtProductVersion>GanttProject 3.4 Beta</txtProductVersion>
    <productName>GanttProject</productName>
    <companyName>BarD Software s.r.o.</companyName>
    <internalName>GanttProject</internalName>
    <originalFilename>ganttproject.exe</originalFilename>
    <trademarks></trademarks>
    <language>ENGLISH_US</language>
  </versionInfo>
  <messages>
    <startupErr>GanttProject failed to start. Please visit https://help.ganttproject.biz to get support.</startupErr>
    <jreNotFoundErr>This application requires a Java Runtime Environment.</jreNotFoundErr>
    <jreVersionErr>GanttProject needs Java 21+ with JavaFX modules. Please visit https://help.ganttproject.biz to get support.</jreVersionErr>
    <launcherErr>Java Runtime not found or is corrupted. Please visit https://help.ganttproject.biz to get support.
        </launcherErr>
    <instanceAlreadyExistsMsg>An application instance is already running.</instanceAlreadyExistsMsg>
  </messages>
</launch4jConfig>